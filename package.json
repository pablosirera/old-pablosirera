    "highlight.js": "^10.4.1",
    "register-service-worker": "^1.6.2",
    "timeline-vuejs": "1.0.9",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.2",
    "vue-i18n": "^8.14.0",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
